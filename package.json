{
  "name": "fractalflows-frontend",
  "private": true,
  "scripts": {
    "dev": "next dev --port 3001",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prepare": "husky install",
    "node:start": "export $(cat .env) && hardhat node",
    "contracts:compile": "export $(cat .env) && hardhat compile",
    "contracts:test": "export $(cat .env) && hardhat test",
    "contracts:deploy": "export $(cat .env) && hardhat run scripts/deploy.js",
    "contracts:deploy:localhost": "npm run contracts:deploy -- --network localhost",
    "contracts:deploy:goerli": "npm run contracts:deploy -- --network goerli",
    "contracts:verify:goerli": "export $(cat .env) && hardhat verify --network goerli"
  },
  "dependencies": {
    "@apollo/client": "^3.5.6",
    "@emotion/react": "^11.7.1",
    "@emotion/server": "^11.4.0",
    "@emotion/styled": "^11.6.0",
    "@mui/icons-material": "^5.2.5",
    "@mui/lab": "^5.0.0-alpha.62",
    "@mui/material": "^5.2.5",
    "@openzeppelin/contracts": "^4.6.0",
    "@sentry/nextjs": "^7.16.0",
    "@web3modal/core": "^2.0.0-beta.3",
    "@web3modal/ethereum": "^2.0.0-beta.3",
    "@web3modal/ui": "^2.0.0-beta.3",
    "apollo-upload-client": "^17.0.0",
    "d3": "3.5.16",
    "ethers": "^5.7.2",
    "graphql": "^16.2.0",
    "lodash-es": "^4.17.21",
    "next": "12.2.1",
    "notistack": "^2.0.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hook-form": "^7.22.5",
    "sharp": "^0.29.3",
    "siwe": "1.1.6",
    "styled-components": "^5.3.3",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@nomicfoundation/hardhat-chai-matchers": "^1.0.4",
    "@nomiclabs/hardhat-ethers": "^2.0.5",
    "@nomiclabs/hardhat-etherscan": "^3.1.2",
    "@types/apollo-upload-client": "^17.0.2",
    "@types/lodash-es": "^4.17.5",
    "@types/node": "17.0.1",
    "@types/react": "17.0.37",
    "chai": "^4.3.6",
    "eslint": "8.5.0",
    "eslint-config-next": "12.0.7",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "ethereum-waffle": "^3.4.4",
    "hardhat": "^2.10.0",
    "hardhat-gas-reporter": "^1.0.8",
    "husky": "^8.0.2",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "solhint": "^3.3.7",
    "typescript": "4.5.4"
  }
}
